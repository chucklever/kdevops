[ext4_2k]
TEST_DEV={{ fstests_test_dev }}
SCRATCH_DEV_POOL="{{ fstests_scratch_devpool }}"
MKFS_OPTIONS='-F -b 2k'

[ext4_4k]
TEST_DEV={{ fstests_test_dev }}
SCRATCH_DEV_POOL="{{ fstests_scratch_devpool }}"
MKFS_OPTIONS='-F -b 4k'

[ext4_advanced_features]
TEST_DEV={{ fstests_test_dev }}
